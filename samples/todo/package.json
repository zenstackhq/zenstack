{
    "name": "todo",
    "version": "0.6.0-pre.19",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "pnpm generate && next build",
        "start": "next start",
        "lint": "next lint",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate deploy",
        "db:reset": "prisma migrate reset",
        "db:browse": "prisma studio",
        "generate": "pnpm install && NODE_PATH=node_modules npx zenstack generate",
        "vercel-build": "pnpm build && pnpm db:deploy",
        "deps-local": "npm i -D ../../packages/schema/dist && npm i ../../packages/runtime/dist ../../packages/next/dist zod zod-validation-error swr decimal.js",
        "deps-latest": "npm rm zod zod-validation-error swr decimal.js && npm i -D zenstack@latest && npm i @zenstackhq/runtime@latest @zenstackhq/next@latest",
        "deps-dev": "npm rm zod zod-validation-error swr decimal.js && npm i -D zenstack@dev && npm i @zenstackhq/runtime@dev @zenstackhq/next@dev"
    },
    "dependencies": {
        "@heroicons/react": "^2.0.12",
        "@next-auth/prisma-adapter": "^1.0.5",
        "@prisma/client": "4.7.0",
        "@tanstack/react-query": "^4.20.2",
        "@trpc/client": "^10.5.0",
        "@trpc/next": "^10.5.0",
        "@trpc/react-query": "^10.5.0",
        "@trpc/server": "^10.5.0",
        "@zenstackhq/next": "workspace:*",
        "@zenstackhq/runtime": "workspace:*",
        "@zenstackhq/react": "workspace:*",
        "@zenstackhq/trpc": "workspace:*",
        "babel-plugin-superjson-next": "^0.4.5",
        "bcryptjs": "^2.4.3",
        "daisyui": "^2.31.0",
        "moment": "^2.29.4",
        "nanoid": "^4.0.0",
        "next": "^12.3.1",
        "next-auth": "^4.15.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-toastify": "^9.0.8",
        "superjson": "^1.12.0",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^14.17.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "autoprefixer": "^10.4.12",
        "eslint": "^7.19.0",
        "eslint-config-next": "12.3.1",
        "postcss": "^8.4.16",
        "prisma": "4.7.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.6.2",
        "zenstack": "workspace:*"
    }
}
