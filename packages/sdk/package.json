{
    "name": "@zenstackhq/sdk",
    "version": "1.0.0-beta.18",
    "description": "ZenStack plugin development SDK",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint src --ext ts",
        "build": "pnpm lint && pnpm clean && tsc && copyfiles ./package.json ./LICENSE ./README.md dist && pnpm pack dist --pack-destination '../../../.build'",
        "watch": "tsc --watch",
        "prepublishOnly": "pnpm build",
        "publish-dev": "pnpm build && pnpm publish --tag dev"
    },
    "publishConfig": {
        "directory": "dist",
        "linkDirectory": true
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@prisma/generator-helper": "^5.0.0",
        "@prisma/internals": "4.10.0",
        "@prisma/internals-v5": "npm:@prisma/internals@^5.0.0",
        "@zenstackhq/language": "workspace:*",
        "@zenstackhq/runtime": "workspace:*",
        "prettier": "^2.8.3",
        "semver": "^7.3.8",
        "ts-morph": "^16.0.0"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/semver": "^7.3.13",
        "copyfiles": "^2.4.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.4"
    }
}
